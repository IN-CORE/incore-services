@prefix unit:    <http://incore.ncsa.illinois.edu/semantic/elements/units/> .
@prefix def:     <http://incore.ncsa.illinois.edu/semantic/schema/units/> .
@prefix rdf:     <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix prefix:  <http://incore.ncsa.illinois.edu/semantic/elements/prefixes/> .
@prefix dim:     <http://incore.ncsa.illinois.edu/semantic/elements/dimensions/> .
@prefix rdfs:    <http://www.w3.org/2000/01/rdf-schema#> .

unit:radian
      a       def:CoherentDerivedUnit ;
      rdfs:label "radian"@en ;
      def:applicable_prefixes
              (prefix:yocto prefix:zepto prefix:atto prefix:femto prefix:pico prefix:nano prefix:micro prefix:milli prefix:centi prefix:deci) ;
      def:dimension_type dim:angle ;
      def:plural "radians"@en ;
      def:symbol "rad" ;
      def:unit_system "SI" ;
      def:utf_plural "radians"@en ;
      def:utf_symbol "rad" .

unit:metre
      a       def:PrefixableUnit ;
      rdfs:label "metre"@en ;
      def:applicable_prefixes
              (prefix:yotta prefix:zetta prefix:exa prefix:peta prefix:tera prefix:giga prefix:mega prefix:kilo prefix:hecto prefix:deka prefix:yocto prefix:zepto prefix:atto prefix:femto prefix:pico prefix:nano prefix:micro prefix:milli prefix:centi prefix:deci) ;
      def:dimension_type dim:length ;
      def:plural "metres"@en ;
      def:symbol "m" ;
      def:unit_system "SI" ;
      def:utf_plural "metres"@en ;
      def:utf_symbol "m" .
